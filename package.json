{
  "name": "fullstack",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "start": "run-p client:dev service:dev middleware:dev service:start middleware:start",
    "build": "run-p client:build middleware:build service:build",
    "client:dev": "webpack-dev-server --config build/webpack.dev.js --port 8089",
    "service:dev": "run-p -l serviceUsers:dev serviceProducts:dev",
    "service:start": "run-p -l serviceUsers:start serviceProducts:start",
    "middleware:dev": "babel middleware --watch --out-dir middlewareDist",
    "middleware:start": "supervisor middlewareDist/index.js",
    "serviceUsers:dev": "babel serviceUsers --watch --out-dir serviceUsersDist",
    "serviceUsers:start": "supervisor serviceUsersDist/index.js",
    "serviceProducts:dev": "babel serviceProducts --watch --out-dir serviceProductsDist",
    "serviceProducts:start": "supervisor serviceProductsDist/index.js",
    "client:build": "webpack --progress --config build/webpack.build.js",
    "service:build": "run-p serviceUsers:build serviceProducts:build",
    "middleware:build": "babel middleware --out-dir middlewareDist",
    "serviceUsers:build": "babel serviceUsers --out-dir serviceUsersDist",
    "serviceProducts:build": "babel serviceProducts --watch --out-dir serviceProductsDist",
    "client:analysis": "cross-env NODE_ENV=production analysis=true webpack --progress --config build/webpack.build.js"
  },
  "description": "micro-frontend + middleware + multi-service",
  "repository": {
    "type": "git",
    "url": "https://github.com/xianweics"
  },
  "license": "ISC",
  "dependencies": {
    "@koa/router": "^9.0.1",
    "axios": "^0.19.2",
    "http-proxy-middleware": "^1.0.4",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-static": "^5.0.0",
    "log4js": "^6.3.0",
    "mysql": "^2.18.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "single-spa": "^5.5.1",
    "single-spa-angularjs": "^3.3.0",
    "single-spa-react": "^2.14.0",
    "single-spa-vue": "^1.8.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@sentry/browser": "^5.15.5",
    "@sentry/integrations": "^5.15.5",
    "@sentry/webpack-plugin": "^1.11.1",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "filemanager-plugin": "^2.3.3",
    "html-webpack-plugin": "^4.3.0",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.1",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
